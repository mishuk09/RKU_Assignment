    "import pymysql\n",
    "from tkinter import *\n",
    "import datetime\n",
    "\n",
    "# Connect to the database\n",
    "db = pymysql.connect(host=\"localhost\", user=\"root\", password=\"\", database=\"rku_attendence_admin\")\n",
    "cursor = db.cursor()\n",
    "\n",
    "# Retrieve student data from the database\n",
    "cursor.execute(\"SELECT name, roll FROM it_students\")\n",
    "it_students = cursor.fetchall()\n",
    "\n",
    "cursor.execute(\"SELECT name, roll FROM ce_students\")\n",
    "ce_students = cursor.fetchall()\n",
    "\n",
    "# Create a new table to store attendance data\n",
    "cursor.execute(\"CREATE TABLE IF NOT EXISTS attendance1 (id INT AUTO_INCREMENT PRIMARY KEY, student_id INT, date DATE, status VARCHAR(10))\")\n",
    "\n",
    "# Build the GUI\n",
    "root = Tk()\n",
    "root.geometry(\"600x400\")\n",
    "root.title(\"Attendance Management System\")\n",
    "\n",
    "# Create a listbox to display students\n",
    "listbox = Listbox(root, width=50)\n",
    "listbox.pack(pady=20)\n",
    "\n",
    "for student in it_students:\n",
    "    listbox.insert(END, f\"{student[0]} - {student[1]} (IT)\")\n",
    "\n",
    "for student in ce_students:\n",
    "    listbox.insert(END, f\"{student[0]} - {student[1]} (CE)\")\n",
    "\n",
    "# Create a function to mark attendance\n",
    "def mark_attendance():\n",
    "    selected = listbox.curselection()\n",
    "    if selected:\n",
    "        student = listbox.get(selected[0])\n",
    "        student_id = int(student.split(\" - \")[1])\n",
    "        date = datetime.date.today()\n",
    "        status = \"Present\"\n",
