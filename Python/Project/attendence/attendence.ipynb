    "import mysql.connector\n",
    "from tkinter import *\n",
    "\n",
    "# Create a connection to the database\n",
    "mydb = mysql.connector.connect(\n",
    "  host=\"localhost\",\n",
    "  user=\"root\",\n",
    "  password=\"\",\n",
    "  database=\"rku_attendence_admin\"\n",
    ")\n",
    "\n",
    "# Create a cursor object\n",
    "mycursor = mydb.cursor()\n",
    "\n",
    "# Create the IT students table if it doesn't exist\n",
    "mycursor.execute(\"CREATE TABLE IF NOT EXISTS it_students (id INT AUTO_INCREMENT PRIMARY KEY, name VARCHAR(255), attendance VARCHAR(255))\")\n",
    "\n",
    "# Create the CE students table if it doesn't exist\n",
    "mycursor.execute(\"CREATE TABLE IF NOT EXISTS ce_students (id INT AUTO_INCREMENT PRIMARY KEY, name VARCHAR(255), attendance VARCHAR(255))\")\n",
    "\n",
    "# Create the GUI\n",
    "root = Tk()\n",
    "root.title(\"Attendance System\")\n",
    "\n",
    "# Add a label for IT students\n",
    "it_label = Label(root, text=\"IT Students\")\n",
    "it_label.grid(row=0, column=0)\n",
    "\n",
    "# Add a label for CE students\n",
    "ce_label = Label(root, text=\"CE Students\")\n",
    "ce_label.grid(row=0, column=1)\n",
    "\n",
    "# Add a label for the attendance status\n",
    "status_label = Label(root, text=\"\")\n",
    "status_label.grid(row=2, columnspan=2)\n",
    "\n",
    "# Add a list of IT students\n",
    "it_listbox = Listbox(root)\n",
    "it_listbox.grid(row=1, column=0)\n",
    "\n",
    "# Add a list of CE students\n",
    "ce_listbox = Listbox(root)\n",
    "ce_listbox.grid(row=1, column=1)\n",
    "\n",
    "# Add IT students to the list\n",
    "mycursor.execute(\"SELECT name FROM it_students\")\n",
    "for x in mycursor:\n",
    "    it_listbox.insert(END, x)\n",
    "\n",
    "# Add CE students to the list\n",
    "mycursor.execute(\"SELECT name FROM ce_students\")\n",
    "for x in mycursor:\n",
    "    ce_listbox.insert(END, x)\n",
    "\n",
    "# Create a function to update attendance\n",
    "def update_attendance():\n",
    "    # Get the selected IT students\n",
    "    it_selected = it_listbox.curselection()\n",
    "    it_students = [it_listbox.get(i) for i in it_selected]\n",
    "\n",
    "    # Get the selected CE students\n",
    "    ce_selected = ce_listbox.curselection()\n",
    "    ce_students = [ce_listbox.get(i) for i in ce_selected]\n",
    "\n",
    "    # Update the attendance of IT students\n",
    "    for student in it_students:\n",
    "        sql = \"INSERT INTO it_students (name, attendance) VALUES (%s, %s)\"\n",
    "        val = (student, \"Present\")\n",
    "        mycursor.execute(sql, val)\n",
    "        mydb.commit()\n",
    "\n",
    "    # Update the attendance of CE students\n",
    "    for student in ce_students:\n",
    "        sql = \"INSERT INTO ce_students (name, attendance) VALUES (%s, %s)\"\n",
    "        val = (student, \"Present\")\n",
    "        mycursor.execute(sql, val)\n",
    "        mydb.commit()\n",
    "\n",
    "    # Update the status label\n",
    "    status_label.config(text=\"Attendance updated successfully\")\n",
    "\n",
    "# Add a button to update attendance\n",
    "update_button = Button(root, text=\"Update Attendance\", command=update_attendance)\n",
    "update_button.grid(row=3, columnspan=2)\n",
    "\n",
    "# Run the GUI\n",
    "root.mainloop()\n"
   ]
